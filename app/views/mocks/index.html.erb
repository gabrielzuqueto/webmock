<p id="notice"><%= notice %></p>

<h1>Listing Mocks</h1>

<table>
  <thead>
    <tr>
      <th>Path</th>
      <th>Status</th>
      <th>Method</th>
      <th>headers</th>
      <th>data_type</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mocks.each do |mock| %>
      <tr>
        <td><%= mock.path %></td>
        <td><%= mock.status %></td>
        <td><%= mock.method %></td>
        <td><%= mock.headers %></td>
        <td><%= mock.data_type %></td>
        <td><%= mock.body %></td>
        <td><%= link_to 'Show', mock %></td>
        <td><%= link_to 'Edit', edit_mock_path(mock) %></td>
        <td><%= link_to 'Destroy', mock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mock', new_mock_path %>
